{
  "name": "angular-tailwind",
  "displayName": "Senasa Tupa",
  "version": "0.5.0",
  "description": "Angular & Tailwind CSS Admin Dashboard Starter Kit, Free and Open Source",
  "homepage": "https://github.com/luciano-work/angular-tailwind#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luciano-work/angular-tailwind.git"
  },
  "keywords": [
    "angular",
    "tailwind",
    "starter-kit",
    "tailwind-template",
    "dark mode",
    "open source"
  ],
  "author": "Luciano Oliveira <me@lanno.dev>",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "build-test": "ng build --configuration=test",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "e2e-ui": "npx playwright test --ui",
    "prettier": "prettier --config ./.prettierrc --write \"src/{app,environments}/**/*{.ts,.html,.scss,.json}\"",
    "prettier:verify": "prettier --config ./.prettierrc --check \"src/{app,environments}/**/*{.ts,.html,.scss,.json}\"",
    "prettier:staged": "pretty-quick --staged",
    "lint": "ng lint",
    "compodoc": "npx compodoc -p tsconfig.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.2.2",
    "@angular/cdk": "~17.3.10",
    "@angular/common": "^17.2.2",
    "@angular/compiler": "^17.2.2",
    "@angular/core": "^17.2.2",
    "@angular/forms": "^17.2.2",
    "@angular/material": "^17.3.10",
    "@angular/platform-browser": "^17.2.2",
    "@angular/platform-browser-dynamic": "^17.2.2",
    "@angular/router": "^17.2.2",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "@visurel/iconify-angular": "^11.0.0",
    "angular-svg-icon": "^13.0.0",
    "apexcharts": "^3.35.3",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "iconify": "^1.4.0",
    "luxon": "^3.5.0",
    "moment": "^2.30.1",
    "ng-apexcharts": "^1.7.1",
    "ngx-bootstrap": "^12.0.0",
    "ngx-pagination": "^6.0.3",
    "rxjs": "~7.4.0",
    "sweetalert2": "^11.14.5",
    "tslib": "^2.3.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.2.0",
    "@angular/cli": "^17.2.0",
    "@angular/compiler-cli": "^17.2.2",
    "@compodoc/compodoc": "^1.1.25",
    "@playwright/test": "^1.42.1",
    "@types/crypto-js": "^4.2.2",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~3.10.0",
    "@types/luxon": "^3.4.2",
    "@types/node": "^12.11.1",
    "autoprefixer": "^10.4.7",
    "cz-conventional-changelog": "^3.3.0",
    "jasmine-core": "~3.10.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwind-scrollbar": "^1.3.1",
    "tailwindcss": "^3.1.6",
    "typescript": "~5.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
